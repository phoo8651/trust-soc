cd /home/agent
source etc/.env
set -a

curl -X POST "http://127.0.0.1:${LISTEN_PORT}/v1/logs" \
  -H "Authorization: Bearer ${LOCAL_TOKEN}" \
  -H "Content-Type: application/json" \
  -d '{
    "resourceLogs": [{
      "scopeLogs": [{
        "logRecords": [{
          "timeUnixNano": 1732990000000000000,
          "body": { "stringValue": ingest/logs 수동 테스트 로그입니다." }
        }]
      }]
    }]
  }'
