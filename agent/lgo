  -H "Authorization: Bearer dev_agent_token" \
  -H "Content-Type: application/json" \
  -d '{
        "resourceLogs": [{
          "scopeLogs": [{
            "logRecords": [{
              "timeUnixNano": 1732900000000000000,
              "body": {
                "stringValue": "✅ OTLP → ingest/logs 테스트 로그입니다."
              }
            }]
          }]
        }]
      }'
